# Redirections https adiyongroup.com
RewriteEngine On
RewriteCond %{HTTP:X-Forwarded-Proto} !https
RewriteCond %{HTTPS} !on
RewriteCond %{HTTP_HOST} ^(?:www\.)?adiyongroup\.com(?:.*)$ [nc]
